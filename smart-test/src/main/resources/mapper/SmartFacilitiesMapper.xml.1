<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.smart.mapper.SmartFacilitiesMapper">
  <resultMap id="BaseResultMap" type="com.briup.smart.bean.SmartFacilities">
    <id column="f_id" jdbcType="INTEGER" property="fId" />
    <result column="f_action" jdbcType="VARCHAR" property="fAction" />
    <result column="f_protocol" jdbcType="VARCHAR" property="fProtocol" />
    <result column="f_name" jdbcType="VARCHAR" property="fName" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from smart_facilities
    where f_id = #{fId,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="com.briup.smart.bean.SmartFacilities">
    insert into smart_facilities (f_id, f_action, f_protocol, 
      f_name)
    values (#{fId,jdbcType=INTEGER}, #{fAction,jdbcType=VARCHAR}, #{fProtocol,jdbcType=VARCHAR}, 
      #{fName,jdbcType=VARCHAR})
  </insert>
  <update id="updateByPrimaryKey" parameterType="com.briup.smart.bean.SmartFacilities">
    update smart_facilities
    set f_action = #{fAction,jdbcType=VARCHAR},
      f_protocol = #{fProtocol,jdbcType=VARCHAR},
      f_name = #{fName,jdbcType=VARCHAR}
    where f_id = #{fId,jdbcType=INTEGER}
  </update>
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select f_id, f_action, f_protocol, f_name
    from smart_facilities
    where f_id = #{fId,jdbcType=INTEGER}
  </select>
  <select id="selectAll" resultMap="BaseResultMap">
    select f_id, f_action, f_protocol, f_name
    from smart_facilities
  </select>
</mapper>